{
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "parser": "vue-eslint-parser",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended",
    "@vue/typescript/recommended",
    "@vue/prettier",
    "@vue/prettier/@typescript-eslint"
  ],
  "rules": {
    "lines-between-class-members": 1
  }
}